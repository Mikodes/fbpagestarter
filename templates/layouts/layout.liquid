<!doctype html>
<html lang="en">
  <head>
    <title>{% block title %}FBPAGESTARTER - a Front-end kit for facebook pages{% endblock %}</title>
    <link rel="shortcut icon" type="image/ico" href="/favicon.png" />
    {{ "fbpagestarter.css" | asset_url | stylesheet_tag }}
    {% block styles %}{% endblock %}
  </head>
  <body>
    <div id="application" class="page-container">
      {% block content %}{% endblock %}
      {% include "footer" %}
    </div>

    {% block scripts %}{% endblock %}
    {{ "lib/jquery-1.10.1.min.js" | asset_url | script_tag }}
    {{ "lib/jquery.fbpagestarter.js" | asset_url | script_tag }}
    {{ "app.js" | asset_url | script_tag }}
    <script src="https://google-code-prettify.googlecode.com/svn/loader/run_prettify.js"></script>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-41928698-1', 'fbdoctor.com');
      ga('send', 'pageview');

    </script>
  </body>
</html>


